{
    "name": "ufutx/laravel-comment",
    "type": "library",
    "description": "Just another comment system for laravel projects.",
    "keywords": [
        "ufutx",
        "laravel-comment"
    ],
    "homepage": "https://github.com/glore/laravel-comment",
    "license": "MIT",
    "authors": [
        {
            "name": "Mehmet Aydin Bahadir zeng glore",
            "email": "zglore@163.com",
            "homepage": "http://www.ufutx.com",
            "role": "Software Engineer"
        }
    ],
    "require": {
        "illuminate/database": "5.4.*",
        "php" : ">=5.6.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit" : "5.*",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.4",
        "orchestra/database": "3.4.x@dev"
    },
    "autoload": {
        "psr-4": {
            "Ufutx\\LaravelComment\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ufutx\\LaravelComment\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
